            <Source Path="DBMigration" DbContext="CapstoneProject.DAL.DataContext, CapstoneProject" MigrationConfiguration="CapstoneProject.DataMigrations.DataConfig, CapstoneProject" Origin="Convention" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DataContext-Web.config Connection String">
