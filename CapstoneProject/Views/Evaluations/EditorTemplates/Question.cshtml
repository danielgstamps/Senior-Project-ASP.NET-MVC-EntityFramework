@model CapstoneProject.Models.Question
<div>
    @Html.HiddenFor(x => x.QuestionID)
    <h3> @Model.QuestionID. @Model.QuestionText </h3>
    @foreach (var a in Model.Answers)
    {
        <p>
            @Html.RadioButtonFor(b => b.SelectedAnswer, a.AnswerID)  @a.AnswerText
        </p>
    }
</div>